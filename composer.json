{
    "name": "yiisoft/yii-rest",
    "description": "Yii framework REST extension",
    "keywords": ["yii", "framework", "rest", "api"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-rest/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2-rest"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com",
            "homepage": "http://www.yiiframework.com/"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru",
            "homepage": "http://rmcreative.ru/"
        }
    ],
    "require": {
        "yiisoft/yii-core": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Rest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Rest\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "web": "config/web.php",
            "tests": "$web"
        }
    }
}
